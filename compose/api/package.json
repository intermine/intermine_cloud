{
  "name": "@intermine/compose-rest-client",
  "version": "1.1.0",
  "dependencies": {
    "axios": "^0.21.2"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@redocly/openapi-cli": "^1.0.0-beta.65",
    "@openapitools/openapi-generator-cli": "^2.2.5",
    "@types/node": "^12.11.5",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "start": "openapi preview-docs",
    "build": "openapi bundle -o dist",
    "test": "openapi lint",
    "generate": "npx openapi-generator-cli generate -i dist.yaml -o client -g typescript-axios -c openapitools.json && mkdir -p client/dist",
    "copy": "cp dist.yaml client/dist/ && cp package.json client/package.json",
    "ts": "tsc index.ts -d --outDir dist/",
    "prepublishOnly": "npm run ts"
  },
  "files": [
    "dist/**/*"
  ]
}
